$(function() {
	$('.button').click(function() {
		$.ajax({
			url: 'https://api.github.com/search/users',
			type: 'GET',
			dataType: 'json',
			data: {q: $('.box').val()}
		})
		.done(function() {
			console.log("success");
		})
		.fail(function() {
			console.log("error");
		})
		.always(function() {
			console.log("complete");
		});
		
	});
});